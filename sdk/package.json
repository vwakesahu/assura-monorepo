{
  "name": "assura-sdk",
  "version": "1.0.1",
  "description": "TypeScript SDK and Solidity contracts for Assura Network - A compliance layer for blockchain applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./abis": {
      "import": "./dist/abis/index.mjs",
      "require": "./dist/abis/index.js",
      "types": "./dist/abis/index.d.ts"
    },
    "./contracts": "./contracts"
  },
  "files": [
    "dist",
    "contracts",
    "README.md",
    "LICENSE",
    "PACKAGE_STRUCTURE.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "assura",
    "compliance",
    "blockchain",
    "ethereum",
    "web3",
    "kyc",
    "attestation",
    "tee"
  ],
  "author": "Assura Network",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/assura-network/assura-monorepo.git",
    "directory": "sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "viem": "^2.30.0",
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@types/node": "^22.8.5",
    "tsup": "^8.0.0",
    "typescript": "^5.8.0"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "viem": {
      "optional": false
    }
  }
}
